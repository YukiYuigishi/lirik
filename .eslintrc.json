{
   "env": {
      "browser": true,
      "es2021": true
   },
   "extends": [
      "airbnb-base",
      "airbnb-typescript/base",
      "prettier"
   ],
   "parser": "@typescript-eslint/parser",
   "parserOptions": {
      "ecmaVersion": "latest",
      "sourceType": "module",
      "project": "./tsconfig.json"
   },
   "plugins": [
      "@typescript-eslint"
   ],
   "rules": {
      "quotes": [
         "error",
         "double"
      ],
      "no-console": "off",
      "import/no-unresolved": "off",
      "import/extensions": "off"
   }
}
